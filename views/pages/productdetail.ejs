<% include ../partials/header %>
<div class="container">
    <h1> <%= detail.brand %></h1>

    <div class="form-group">
        <label for=" ">ID></label>
        <input type="text" value="<%= detail.ID %>" readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
        <label for=" ">Brand></label>
        <input type="text" value="<%= detail.brand %>" readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
        <label for=" ">Generation></label>
        <input type="text" value="<%= detail.generation %>" readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <div class="form-group">
        <label for=" " >Unit Price></label>
        <input type="text" value="<%= detail.price %>" readonly class="form-control" name="" id=""
            aria-describedby="helpId" placeholder="">
        <small id="helpId" class="form-text text-muted">Help text</small>
    </div>
    <a name="" id="" class="btn btn-primary" href="/products" role="button">Back to Product Lit</a>
</div>

<% include ../partials/footer %>